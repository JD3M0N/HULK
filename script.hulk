

function factorial_iterative(n) => {
    let result = 1 in
    let i = 1 in {
        while (i <= n) {
            result := result * i;
            i := i + 1;
        };
        result;
    };
};

function sum_range(start, end) => {
    let sum = 0 in
    let current = start in {
        while (current <= end) {
            sum := sum + current;
            current := current + 1;
        };
        sum;
    };
};

print(factorial_iterative(5));
print(sum_range(1, 10));
